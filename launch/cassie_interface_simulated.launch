<?xml version="1.0" ?>
<launch>

<include file="$(find cassie_interface)/launch/cassie_world.launch"/>

<rosparam>
    use_sim_time: true
    cassie:
        is_simulation: true
        dt: 0.0005
        log_estimation: true
        log_controller: true
        interface:
            use_ekf: true
            safe_torque_limit: '[90, 60, 90, 200, 40, 90, 60, 90, 200, 40]'
            heelspring_solver:
                zero_springs_on_startup: false
                num_zeroing_samples: 2000
            contact_classifier:
                use_sigmoid: false
                sigmoid_A: 0.30
                sigmoid_B: 30
                sigmoid_power: 1
                linear_lb: 40
                linear_ub: 90
                lowpass_dt_cutoff: 0.005
            kinematics_velocity:
                lowpass_vx_dt_cutoff: 0.06
                lowpass_vy_dt_cutoff: 0.06
                lowpass_vz_dt_cutoff: 0.06
            contact_ekf:
                apply_post_filter: false
                post_filter_dt_cutoff_x: 0.03
                post_filter_dt_cutoff_y: 0.03
                post_filter_dt_cutoff_z: 0.03
                gyro_noise_std: 0.000065
                gyro_bias_noise_std: 0.0001
                accel_noise_std: 0.0045
                accel_bias_noise_std: 0.0001
                contact_noise_std: 0.0001
                encoder_noise_std: 0.000175
                contact_std: 0.001
                contact_yaw_std: 0.001
                prior_base_rotation_std: 0.000001
                prior_base_position_std: 0.0001
                prior_base_velocity_std: 0.001
                prior_contact_position_std: 0.0025
                prior_gyro_bias_std: 0.00001
                prior_accel_bias_std: 0.0001
                prior_forward_kinematics_std: 0.003
                prior_contact_yaw_std: 0.03
        locomotion_control:
            dt: 0.001
            standing:
                use_qp: true
                use_lateral_comp: true
                Kp_lateral_compensator: 3.0
                Kd_lateral_compensator: 0.04
                kp: '[200., 100., 300., 400., 50.]'
                kd: '[5., 2., 6., 8., 2.]'
                pose_command:
                    x_com_offset: -0.01
                    x_lowpass_dt_cutoff: 0.75
                    height_lowpass_dt_cutoff: 1.5
                    pitch_lowpass_dt_cutoff: 2.0
                    lateral_lowpass_dt_cutoff: 2.0
                    height_lb: -0.90
                    height_ub: -0.40
                    lateral_lb: -0.75
                    lateral_ub: 0.75
                    pitch_lb: 0
                    pitch_ub: 0
                inverse_kinematics:
                    ik_xtol: 0.00000001
                    ik_iter_limit: 10
                qp:
                    clf_use_Vdot_cost: true
                    clf_use_inequality: false
                    clf_use_task_pd: false
                    clf_gam: 0.14
                    res_clf_ep: 0.3
                    Pdiag: '[3570.7142, 3570.7142, 2786.6895, 1538.2423, 1538.2423, 862.40613, 71.414284, 71.414284, 63.931041, 39.717246, 39.717246, 23.918842]'
                    Poffdiag: '[50, 50, 43.589, 38.7298, 38.7298, 36.0555]'
                    kp: '[300, 500, 200, 400, 200, 100]'
                    kd: '[12, 15, 8, 10, 4, 4]'
                    nQPIter: 30
                    reg_ddq: 0.1
                    reg_u: 1.1
                    reg_achilles: 0.000001
                    reg_rigid: 0.00001
                    reg_fx: 0.02
                    reg_fy: 0.005
                    reg_fz: 0.000001
                    reg_muy: 0.0001
                    reg_muz: 0.1
                    reg_clf_delta: 10
                    w_u_chatter: 0.001
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
                    w_Vdot: 1
            stepping:
                use_qp: false
                nDomain: 2
                time_scale: 1.0
                x_offset: 0.035
                vx_offset: 0.02
                vy_offset: 0.045
                u_stancetoe_offset: 2.5
                ff_scaling: 1.4
                stoppable_velocity_threshold: 0.2
                use_feedforward: true
                ff_use_rotation: false
                ff_zero_springs: true
                use_contact_switching: true
                grf_KpX: 0
                grf_KpY: 0
                ddq_KdX: 0.50
                ddq_KpY: 8.00
                ddq_KdY: 1.75
                kp: '[500, 500, 200, 300, 200, 300, 200, 200, 50]'
                kd: '[6, 8, 4, 6, 6, 6, 4, 4, 2]'
                gaitlib: 'package://cassie_controllers/gaits/20200904.yaml'
                raibert:
                    phaseThreshold: 0.50
                    KpX: 0.08
                    KdX: 0.15
                    KpY: 0.25
                    KdY: 0.55
                qp:
                    kp: '[400, 400, 400, 600, 600, 600, 400, 400, 50]'
                    kd: '[8, 8, 6, 10, 10, 10, 6, 6, 2]'
                    nQPIter: 30
                    reg_ddq: 0.1
                    reg_u: 0.2
                    reg_achilles: 0.00001
                    reg_rigid: 0.00001
                    reg_fx: 0.0001
                    reg_fy: 0.0001
                    reg_fz: 0.000001
                    reg_muy: 0.000001
                    reg_muz: 0.001
                    reg_clf_delta: 0.0001
                    w_u_chatter: 1
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
</rosparam>

<node name="cassie_interface_node" pkg="cassie_interface" type="cassie_interface_node" args="" respawn="false" />

</launch>
