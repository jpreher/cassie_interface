<?xml version="1.0" ?>
<launch>

<include file="$(find cassie_interface)/launch/cassie_world.launch"/>

<rosparam>
    use_sim_time: true
    cassie:
        is_simulation: true
        dt: 0.0005
        log_estimation: true
        log_controller: true
        interface:
            use_ekf: true
            safe_torque_limit: '[90, 60, 90, 200, 40, 90, 60, 90, 200, 40]'
            heelspring_solver:
                zero_springs_on_startup: false
                num_zeroing_samples: 2000
            contact_classifier:
                use_sigmoid: true
                sigmoid_A: 0.30
                sigmoid_B: 30
                sigmoid_power: 1
                linear_lb: 40
                linear_ub: 90
                lowpass_dt_cutoff: 0.005
            kinematics_velocity:
                lowpass_vx_dt_cutoff: 0.09
                lowpass_vy_dt_cutoff: 0.09
                lowpass_vz_dt_cutoff: 0.09
            contact_ekf:
                apply_post_filter: true
                post_filter_dt_cutoff_x: 0.09
                post_filter_dt_cutoff_y: 0.03
                post_filter_dt_cutoff_z: 0.06
                gyro_noise_std: 0.000065
                gyro_bias_noise_std: 0.0001
                accel_noise_std: 0.0075
                accel_bias_noise_std: 0.0001
                contact_noise_std: 0.001
                encoder_noise_std: 0.0175
                contact_std: 0.001
                contact_yaw_std: 0.001
                prior_base_rotation_std: 0.01
                prior_base_position_std: 0.01
                prior_base_velocity_std: 0.1
                prior_contact_position_std: 0.025
                prior_gyro_bias_std: 0.01
                prior_accel_bias_std: 0.1
                prior_forward_kinematics_std: 0.03
                prior_contact_yaw_std: 0.03
        locomotion_control:
            dt: 0.001
            standing:
                use_qp: true
                use_lateral_comp: true
                Kp_lateral_compensator: 3.0
                Kd_lateral_compensator: 0.04
                kp: '[200., 100., 300., 400., 50.]'
                kd: '[5., 2., 6., 8., 2.]'
                pose_command:
                    x_com_offset: -0.01
                    x_lowpass_dt_cutoff: 0.75
                    height_lowpass_dt_cutoff: 1.5
                    pitch_lowpass_dt_cutoff: 2.0
                    lateral_lowpass_dt_cutoff: 2.0
                    height_lb: -0.90
                    height_ub: -0.40
                    lateral_lb: -0.75
                    lateral_ub: 0.75
                    pitch_lb: 0
                    pitch_ub: 0
                inverse_kinematics:
                    ik_xtol: 0.00000001
                    ik_iter_limit: 10
                qp:
                    clf_use_Vdot_cost: true
                    clf_use_inequality: false
                    clf_use_task_pd: false
                    clf_gam: 0.14
                    res_clf_ep: 0.3
                    Pdiag: '[3570.7142, 3570.7142, 2786.6895, 1538.2423, 1538.2423, 862.40613, 71.414284, 71.414284, 63.931041, 39.717246, 39.717246, 23.918842]'
                    Poffdiag: '[50, 50, 43.589, 38.7298, 38.7298, 36.0555]'
                    kp: '[300, 500, 200, 400, 200, 100]'
                    kd: '[12, 15, 8, 10, 4, 4]'
                    nQPIter: 30
                    reg_ddq: 0.01
                    reg_u: 0.1
                    reg_achilles: 0.000001
                    reg_rigid: 0.00001
                    reg_fx: 0.02
                    reg_fy: 0.005
                    reg_fz: 0.000001
                    reg_muy: 0.0001
                    reg_muz: 0.1
                    reg_clf_delta: 10
                    w_u_chatter: 1.2
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
                    w_Vdot: 1
            stepping:
                use_qp: false
                nDomain: 2
                time_scale: 1.0
                x_offset: 0.035
                ff_scaling: 1.4
                stoppable_velocity_threshold: 0.2
                use_feedforward: true
                ff_use_rotation: false
                ff_zero_springs: true
                use_contact_switching: true
                kpt: '[400, 400, 300, 500, 400, 400, 300, 300, 50]'
                kdt: '[12, 12, 8, 12, 10, 10, 6, 6, 2]'
                kp: '[250, 150, 200, 300, 200, 200, 200, 200, 50]'
                kd: '[3, 2, 6, 8, 8, 8, 6, 6, 2]'
                grf_KpX: -15
                grf_KpY: 0
                gaitlib: 'package://cassie_controllers/gaits/gaitlib_20200715.yaml'
                raibert:
                    phaseThreshold: 0.50
                    KpX: 0.00
                    KdX: 0.00
                    KpY: 0.10
                    KdY: 0.02
                qp:
                    kp: '[400, 400, 400, 600, 600, 600, 400, 400, 25]'
                    kd: '[8, 8, 6, 10, 10, 10, 6, 6, 8]'
                    nQPIter: 30
                    reg_ddq: 0.1
                    reg_u: 0.2
                    reg_achilles: 0.00001
                    reg_rigid: 0.00001
                    reg_fx: 0.0001
                    reg_fy: 0.0001
                    reg_fz: 0.000001
                    reg_muy: 0.000001
                    reg_muz: 0.001
                    reg_clf_delta: 0.0001
                    w_u_chatter: 1
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
</rosparam>

<node name="cassie_interface_node" pkg="cassie_interface" type="cassie_interface_node" args="" respawn="false" />

</launch>
