<?xml version="1.0" ?>
<launch>

<rosparam>
    cassie:
        is_simulation: false
        dt: 0.0005
        log_estimation: true
        log_controller: true
        interface:
            use_ekf: true
            safe_torque_limit: '[90, 60, 90, 200, 40, 90, 60, 90, 200, 40]'
            heelspring_solver:
                zero_springs_on_startup: true
                num_zeroing_samples: 2000
            contact_classifier:
                use_sigmoid: false
                sigmoid_A: 0.30
                sigmoid_B: 30
                sigmoid_power: 1
                linear_lb: 65
                linear_ub: 115
                lowpass_dt_cutoff: 0.005
            kinematics_velocity:
                lowpass_vx_dt_cutoff: 0.09
                lowpass_vy_dt_cutoff: 0.09
                lowpass_vz_dt_cutoff: 0.09
            contact_ekf:
                apply_post_filter: true
                post_filter_dt_cutoff_x: 0.01
                post_filter_dt_cutoff_y: 0.01
                post_filter_dt_cutoff_z: 0.01
                gyro_noise_std: 0.00006
                gyro_bias_noise_std: 0.0001
                accel_noise_std: 0.006
                accel_bias_noise_std: 0.0001
                contact_noise_std: 0.001
                encoder_noise_std: 0.018
                contact_std: 0.001
                contact_yaw_std: 0.001
                prior_base_rotation_std: 0.01
                prior_base_position_std: 0.01
                prior_base_velocity_std: 0.1
                prior_contact_position_std: 0.025
                prior_gyro_bias_std: 0.01
                prior_accel_bias_std: 0.1
                prior_forward_kinematics_std: 0.03
                prior_contact_yaw_std: 0.03
        locomotion_control:
            dt: 0.001
            standing:
                use_qp: true
                use_lateral_comp: true
                Kp_lateral_compensator: 3.0
                Kd_lateral_compensator: 0.04
                kp: '[200., 100., 300., 400., 50.]'
                kd: '[5., 2., 6., 8., 2.]'
                pose_command:
                    x_com_offset: -0.0215
                    x_lowpass_dt_cutoff: 0.75
                    height_lowpass_dt_cutoff: 2.25
                    pitch_lowpass_dt_cutoff: 2.25
                    lateral_lowpass_dt_cutoff: 2.25
                    height_lb: -0.90
                    height_ub: -0.40
                    lateral_lb: -0.90
                    lateral_ub: 0.90
                    pitch_lb: 0
                    pitch_ub: 0
                inverse_kinematics:
                    ik_xtol: 0.00000001
                    ik_iter_limit: 10
                qp:
                    clf_use_Vdot_cost: true
                    clf_use_inequality: false
                    clf_use_task_pd: false
                    clf_gam: 0.0012
                    res_clf_ep: 0.20
                    Pdiag: '[826.5159,826.5159,95.306,97.5402,97.5402,80.88,7.0079,7.0079,4.8985,5.2198,5.2198,4.7754]'
                    Poffdiag: '[36.6025,36.6025,12.3961,14.843,14.843,13.1774]'
                    kp: '[600, 500, 200, 400, 200, 100]'
                    kd: '[18, 15, 8, 10, 4, 4]'
                    nQPIter: 30
                    reg_ddq: 0.001
                    reg_u: 1.8
                    reg_achilles: 0.000001
                    reg_rigid: 0.00001
                    reg_fx: 0.018
                    reg_fy: 0.005
                    reg_fz: 0.000001
                    reg_muy: 0.0001
                    reg_muz: 0.1
                    reg_clf_delta: 10
                    w_u_chatter: 0.01
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
                    w_Vdot: 2.7
            stepping:
                use_qp: false
                nDomain: 2
                time_scale: 1.0
                x_offset: 0.04
                vx_offset: 0.02
                vy_offset: 0.02
                u_stancetoe_offset: 0.025
                ff_scaling: 1.4
                stoppable_velocity_threshold: 0.08
                use_feedforward: true
                ff_use_rotation: false
                ff_zero_springs: true
                use_contact_switching: false
                grf_KpX: 0
                grf_KpY: 0
                ddq_KpX: 0.0
                ddq_KdX: 0.50
                ddq_KpY: 0.0
                ddq_KdY: 0.0
                kp: '[900, 500, 300, 300, 250, 200, 200, 200, 25]'
                kd: '[12, 6, 4, 6, 5, 6, 4, 4, 2]'
                gaitlib: 'package://cassie_controllers/gaits/20200927.yaml'
                raibert:
                    phaseThreshold: 0.50
                    KpX: 0.0
                    KdX: 0.0
                    KpY: 0.10
                    KdY: 0.24
                qp:
                    clf_use_Vdot_cost: false
                    clf_use_inequality: false
                    clf_use_task_pd: true
                    kp: '[600, 600, 300, 1500, 1200, 1000, 400, 400, 250]'
                    kd: '[6, 6, 4, 15, 12, 12, 5, 5, 8]'
                    nQPIter: 100
                    reg_ddq: 0.0001
                    reg_u: 4
                    reg_achilles: 0.000001
                    reg_rigid: 0.00001
                    reg_fx: 0.001
                    reg_fy: 0.001
                    reg_fz: 0.01
                    reg_muy: 0.001
                    reg_muz: 0.01
                    reg_clf_delta: 0.00001
                    w_u_chatter: 0.75
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
                    w_Vdot: 1
</rosparam>

<node name="cassie_interface_node" 
    pkg="cassie_interface" 
    type="cassie_interface_node" 
    args="" 
    respawn="false"
    launch-prefix="nice -n -18 taskset -c 2" 
/>
<!--launch-prefix="nice -n -20 taskset -c 2" -->

<node name="locomotion_node" 
    pkg="cassie_controllers" 
    type="locomotion_node" 
    args="" 
    respawn="false"
    output="screen"
    launch-prefix="nice -n -18 taskset -c 3" 
/>

</launch>
