<?xml version="1.0" ?>
<launch>

<rosparam>
    cassie:
        is_simulation: false
        dt: 0.0005
        log_estimation: true
        log_controller: true
        interface:
            safe_torque_limit: '[90, 60, 90, 200, 40, 90, 60, 90, 200, 40]'
            heelspring_solver:
                zero_springs_on_startup: false
                num_zeroing_samples: 2000
            contact_classifier:
                use_sigmoid: true
                sigmoid_A: 0.30
                sigmoid_B: 30
                sigmoid_power: 1
                linear_lb: 50
                linear_ub: 100
                lowpass_dt_cutoff: 0.005
            kinematics_velocity:
                lowpass_vx_dt_cutoff: 0.095
                lowpass_vy_dt_cutoff: 0.09
                lowpass_vz_dt_cutoff: 0.085
        locomotion_control:
            dt: 0.001
            standing:
                use_qp: true
                use_lateral_comp: true
                Kp_lateral_compensator: 3.0
                Kd_lateral_compensator: 0.04
                kp: '[200., 100., 300., 400., 50.]'
                kd: '[5., 2., 6., 8., 2.]'
                pose_command:
                    x_com_offset: -0.0215
                    x_lowpass_dt_cutoff: 0.75
                    height_lowpass_dt_cutoff: 2.25
                    pitch_lowpass_dt_cutoff: 2.25
                    lateral_lowpass_dt_cutoff: 2.25
                    height_lb: -0.80
                    height_ub: -0.40
                    lateral_lb: -0.90
                    lateral_ub: 0.90
                    pitch_lb: 0
                    pitch_ub: 0
                inverse_kinematics:
                    ik_xtol: 0.00000001
                    ik_iter_limit: 10
                qp:
                    clf_use_Vdot_cost: true
                    clf_use_inequality: false
                    clf_use_task_pd: false
                    clf_gam: 0.0012
                    res_clf_ep: 0.29
                    Pdiag: '[826.5159,826.5159,95.306,97.5402,97.5402,80.88,7.0079,7.0079,4.8985,5.2198,5.2198,4.7754]'
                    Poffdiag: '[36.6025,36.6025,12.3961,14.843,14.843,13.1774]'
                    kp: '[600, 500, 200, 400, 200, 100]'
                    kd: '[18, 15, 8, 10, 4, 4]'
                    nQPIter: 30
                    reg_ddq: 0.001
                    reg_u: 0.1
                    reg_achilles: 0.000001
                    reg_rigid: 0.00001
                    reg_fx: 0.018
                    reg_fy: 0.005
                    reg_fz: 0.000001
                    reg_muy: 0.0001
                    reg_muz: 0.1
                    reg_clf_delta: 10
                    w_u_chatter: 1.35
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
                    w_Vdot: 2.7
            stepping:
                use_qp: false
                nDomain: 2
                time_scale: 1.0
                x_offset: 0.035
                ff_scaling: 1.4
                stoppable_velocity_threshold: 0.2
                use_feedforward: true
                ff_use_rotation: false
                ff_zero_springs: true
                use_contact_switching: true
                use_contact_switching: true
                grf_KpX: 0
                grf_KpY: 0
                kp: '[200, 200, 200, 300, 200, 200, 200, 200, 50]'
                kd: '[8, 8, 6, 10, 8, 8, 6, 6, 2]'
                gaitlib: 'package://cassie_controllers/gaits/gaitlib_20200714.yaml'
                raibert:
                    phaseThreshold: 0.50
                    KpX: 0.10
                    KdX: 0.25
                    KpY: 0.20
                    KdY: 0.15
                qp:
                    kp: '[400, 400, 400, 600, 600, 600, 400, 400, 25]'
                    kd: '[8, 8, 6, 10, 10, 10, 6, 6, 8]'
                    nQPIter: 30
                    reg_ddq: 0.1
                    reg_u: 0.2
                    reg_achilles: 0.00001
                    reg_rigid: 0.00001
                    reg_fx: 0.0001
                    reg_fy: 0.0001
                    reg_fz: 0.000001
                    reg_muy: 0.000001
                    reg_muz: 0.001
                    reg_clf_delta: 0.0001
                    w_u_chatter: 1
                    w_outputs: 1
                    w_hol_achilles: 8
                    w_hol_fixed: 8
                    w_hol_fx: 15
                    w_hol_fy: 15
                    w_hol_fz: 15
                    w_hol_my: 6
                    w_hol_mz: 6
</rosparam>

<node name="cassie_interface_node" 
    pkg="cassie_interface" 
    type="cassie_interface_node" 
    args="" 
    respawn="false"
    launch-prefix="nice -n -20 taskset -c 2" 
/>

<node name="locomotion_node" 
    pkg="cassie_controllers" 
    type="locomotion_node" 
    args="" 
    respawn="false"
    output="screen"
    launch-prefix="nice -n -20 taskset -c 3" 
/>

</launch>
